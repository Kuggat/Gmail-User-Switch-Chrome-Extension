{
    "manifest_version": 3,
    "name": "Switch Gmail User",
    "version": "1.0",
    "permissions": [
        "clipboardWrite",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "switch-user": {
            "suggested_key": {
                "default": "Alt+Q",
                "mac": "Alt+Q"
            },
            "description": "Switch Gmail User"
        }
    }
}